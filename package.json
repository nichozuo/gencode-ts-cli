{
  "name": "@nichozuo/gencode-ts-cli",
  "version": "1.0.0",
  "description": "A CLI tool to generate TypeScript code from OpenAPI specification",
  "main": "dist/index.js",
  "bin": {
    "gencode-ts": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node dist/index.js",
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --format=cjs --minify --minify-identifiers"
  },
  "keywords": [
    "openapi",
    "typescript",
    "code-generator",
    "cli"
  ],
  "author": "zuoge",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.40",
    "@swc/jest": "^0.2.36",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.8.2",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.1"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "node-fetch": "^3.3.2",
    "openapi-typescript": "^7.4.2",
    "radash": "^12.1.0"
  }
}
